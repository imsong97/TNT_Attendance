        <shape
            android:shape="oval">
            <size
                android:height="50dp"
                android:width="50dp"/>
            <corners android:radius="100dp" />
            <solid android:color="@color/color_transparent" />
        </shape>
    </item>
    <item android:start="11dp" android:end="11dp" android:top="11dp" android:bottom="11dp">
